{
  "name": "keep",
  "version": "0.0.1",
  "description": "keep is simple notes that can be written markdown.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "yarn build:development",
    "build:production": "yarn clean && webpack --mode production",
    "build:development": "yarn clean && webpack --mode development",
    "clean": "rimraf dist",
    "test": "yarn lint",
    "lint": "eslint --ext .ts,.tsx src/",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.1.0",
    "remark-breaks": "^1.0.3"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-fix-style-only-entries": "^0.3.0"
  }
}
